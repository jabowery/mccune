#!/bin/csh


if ($#argv != 2) then
	echo "need 2 args: bin-directory ouput-extension"
	exit(1)
endif

set d=$1

set out=$2

# job andrews

$d/fof-mccune -f andrews.in > andrews.$out

# job ALG033+1

$d/fof-mccune -f ALG033+1.in > ALG033+1.$out


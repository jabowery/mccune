# job MGT025+1

$d/fof-mccune -f MGT025+1.in > MGT025+1.$out

# job MGT063+1

# $d/fof-mccune -f MGT063+1.in > MGT063+1.$out

# job SET587+3

$d/fof-mccune -f SET587+3.in > SET587+3.$out

# job SET593+3

